{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:jsx-a11y/recommended",
        "plugin:react/recommended",
        "prettier/react",
        "prettier"
    ],
    "plugins": ["react","import","jsx-a11y","react-hooks"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/prop-types":2,
        "no-console":1,
        "react-hooks/rules-of-hooks": 2,
        "react-hooks/exhaustive-deps": 1
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "settings": {
        "react":{
            "version":"detect"
        }
    }

}